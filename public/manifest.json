{
  "manifest_version": 2,
  "name": "Valinator",
  "author": "Wais Ibrahim",
  "version": "1.0.1",
  "description": "Valinate the val",
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "logo192.png",
    "default_popup": "index.html",
    "default_title": "Valinator"
  },
  "permissions": [
    "tabs",
    "activeTab"
  ]
}